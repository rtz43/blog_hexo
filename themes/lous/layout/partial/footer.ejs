<footer>
    <div class="footer-container">
        <div class="footer-meta">
            <% if (theme.copyright) { %>
                <div class="footer-meta-copyright">
                    &copy; <%= date(new Date(), 'YYYY') %> <%= theme.copyright.name || config.author %>.
                </div>
            <% } %>
            <% if (theme.license) { %>
                <div class="footer-meta-licenese">
                    Licensed under <a href="<%- url_for(theme.license.url) %>"><%= theme.license.name %></a>
                </div>
            <% } %>
            <% if (theme.rss) { %>
                <div class="footer-meta-rss">
                    <a href="<%- url_for(theme.rss.path) %>"><i class="fa-solid fa-rss"></i></a>
                </div>
            <% } %>
        </div>
    </div>
	<div class="footer-meta">
		<i class="fab fa-pied-piper" style=""></i><a target="_blank" rel="noopener" href="http://beian.miit.gov.cn/">苏ICP备17043301号-1</a> 

        <a style="text-indent:1em" target="_blank" href="http://www.beian.gov.cn/portal/registerSystemInfo?recordcode=32010602010279">
            <img src="/img/police.png" style="height:14px;margin-left:-1px;margin-right:-1px;margin-top:1.2px">苏公网安备 32010602010279号
        </a>
    </div>
	<div style="font-size: 11px;color: #999;margin: 0 10px;width: 100%;">
        Supported by<a target="_blank" rel="noopener" href="https://www.upyun.com/?utm_source=lianmeng&amp;utm_medium=referral">
            <img src="/img/upyun.png" style="vertical-align: middle;width: 60px;">
        </a>
    </div>
</footer>
